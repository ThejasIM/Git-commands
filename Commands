git config --global user.name ""
git config --global user.email ""


git clone <link>

git status
git add .
git commit -m ""
git push origin main
git pull origin main


########To create new repo from git########

//create folder
git init
//make changes in the folder
git add .
git commit -m ""
git remote add origin <link>    // link from new repo
git remote -v                   // verfication
git branch                      //branch name
git branch -M main              //rename branch
git push origin main

//////////////////////////////////////

git log --oneline

/////////////////////////////////////

git diff        // diff in current vs previous commit
git stash       // when you want to save changes temporarily
        git stash list
        git stash clear     // to delete list
        git stash apply x   // x is the no from the list
        git stash pop       // to get the last saved back


///////////////// RESTORE /////////////////////

git log --oneline 
git restore <filename>          //restores to previous commit
git restore --staged <filename> //restore from staged to working directory